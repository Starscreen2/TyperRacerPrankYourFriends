{
    "manifest_version": 2,
    "name": "AutoTyper",
    "version": "1.0",
    "description": "Automatically types text with natural intervals and mistakes.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery.min.js", "contentScript.js"]
        }
    ]
}
